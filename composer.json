{
    "name": "illuminated/wikipedia-grabber",
    "description": "Grab Wikipedia (or another MediaWiki) page in Laravel.",
    "keywords": ["laravel", "wiki", "wikipedia", "mediawiki", "grabber", "parser"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dmitry-ivanov/laravel-wikipedia-grabber/issues",
        "source": "https://github.com/dmitry-ivanov/laravel-wikipedia-grabber"
    },
    "authors": [{
        "name": "Dmitry Ivanov",
        "email": "dmitry.g.ivanov@gmail.com"
    }],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Wikipedia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminated\\Wikipedia\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illuminated\\Wikipedia\\WikipediaGrabberServiceProvider"
            ],
            "aliases": {
                "Wikipedia": "Illuminated\\Wikipedia\\Wikipedia",
                "MediaWiki": "Illuminated\\Wikipedia\\MediaWiki"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
